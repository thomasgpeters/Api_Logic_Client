void local fn BuildSubViewsnibview _welcomeView, 		   @"WelcomeView",,  				( _tableViewWidth + (_controlSpacer * 2), _mainWindowHeight - _welcomeViewHeight - ( _controlSpacer * 2) )nibview _projectCreateView,	 @"ProjectCreateView",,		( _tableViewWidth + (_controlSpacer * 2), _mainWindowHeight - _projectCreateViewHeight - ( _controlSpacer * 2) )nibview _projectDetailView,	 @"ProjectDetailView",,		( _tableViewWidth + (_controlSpacer * 2), _mainWindowHeight - _projectDetailViewHeight - ( _controlSpacer * 2) )// Init Toolbar and Viewsnibtoolbar _mainToolbar, @"Toolbar"WindowSetToolbar( _window, _mainToolbar )' set hiddenViewSetHidden( _welcomeView, NO )ViewSetHidden( _projectCreateView, YES )ViewSetHidden( _projectDetailView, YES )WindowMakeFirstResponder( _window, _createProjectCreateBtn )end fnvoid local fn BuildWindow'~'1nibwindow _window, @"MainWindow"WindowCenter( _window )fn TableViewSetCustomCellCallback( _objectTableView, @[ID_KEY,OBJECT_KEY], @fn MyCustomCellCallback, NULL )fn TableViewSizeLastColumnToFit( _objectTableView )ViewSetAutoresizesSubviews( _window, YES )end fn